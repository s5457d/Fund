<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lyq.fund">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fund"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fund">
        <activity android:name="com.lyq.fund.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lyq.fund.ui.activity.AddActivity" />
        <activity android:name="com.lyq.fund.ui.activity.SettingActivity" />
        <activity android:name="com.lyq.fund.ui.activity.ImportActivity" />
        <activity android:name="com.lyq.fund.ui.activity.LevelActivity" />
        <activity android:name="com.lyq.fund.ui.activity.ExportActivity" />
    </application>

</manifest>